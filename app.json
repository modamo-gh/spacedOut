{
  "expo": {
    "name": "spacedOut",
    "slug": "spacedOut",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "infoPlist": {
        "NSUserNotificationUsageDescription": "We need notifications to remind you about important dates.",
        "UIBackgroundModes": [
          "fetch",
          "remote-notification"
        ]
      },
      "bundleIdentifier": "xyz.modamo.spacedOut"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "newArchEnabled": true,
    "plugins": [
      "expo-font"
    ],
    "extra": {
      "eas": {
        "projectId": "2092cbd8-7139-433f-8487-2be7a115387f"
      }
    }
  }
}
